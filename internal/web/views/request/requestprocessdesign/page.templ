package requestprocessdesign

import (
	"algvisual/internal/web/views"
	"algvisual/internal/web/components/layout"
	"algvisual/internal/database"
	"algvisual/internal/web/components/designcard"
)

templ Page(designes []database.Design) {
	<html>
		<head>
			@views.Header()
		</head>
		<body>
			@layout.Layout() {
				for _, d := range designes {
					@designcard.DesignCard(d)
				}
			}
			<script src="/web/js/notifications.js"></script>
		</body>
	</html>
}
